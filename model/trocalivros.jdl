
enum SituacaoItem {
	DISPONIVEL, INDISPONIVEL, TROCADO, EM_NEGOCIACAO
}
entity ItemDesejado {
	nome String
    idGlobal String
    situacao SituacaoItem
}

entity RegistroTroca{
	aceite Boolean
}

entity ItemTroca {
	nome String
    idGlobal String
    situacao SituacaoItem
  
}

entity Troca {
	dataInicio Instant
	dataFim Instant
	resultado TextBlob
	
}

relationship OneToOne {
	RegistroTroca{origem} to ItemTroca
	RegistroTroca{destino} to ItemTroca
	
}

relationship OneToMany{
 Troca to RegistroTroca
}
relationship ManyToOne {
  ItemTroca{user} to User
  ItemDesejado{user} to User
  
}
